<!-- Preconnect and DNS-prefetch to speed up fetching -->
<link rel="preconnect" href="https://scripts.sirv.com" crossorigin>
<link rel="dns-prefetch" href="https://scripts.sirv.com">

<!-- Preload the Sirv script -->
<link rel="preload" href="https://scripts.sirv.com/sirvjs/v3/sirv.js?modules=zoom,video,gallery" as="script">

<!-- Load Sirv script asynchronously and defer execution -->
<script src="https://scripts.sirv.com/sirvjs/v3/sirv.js?modules=zoom,video,gallery" defer></script>

